apiVersion: v1
kind: Secret
metadata:
  name: culturaltrip-secrets
  namespace: culturaltrip
type: Opaque
stringData:
  # Laravel Application Key
  # Generate: docker run --rm --entrypoint php ragna13/culturaltrip:latest artisan key:generate --show
  app-key: "base64:YOUR_APP_KEY_HERE"
  
  # Database Password
  # Should match with mysql-statefulset.yaml MYSQL_PASSWORD
  db-password: "YOUR_DB_PASSWORD_HERE"
